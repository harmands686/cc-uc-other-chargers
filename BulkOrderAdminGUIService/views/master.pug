html
	head
		title= 'Master Orders'
		style
			include style.css
	body
		h1= 'Admin GUI - Master Order Tracking'
	table(class="gridtable")
		tr
			th Id
			th OrderRefNumber
			th CustomerName
			th CustomerAccountId
			th OrderStatus
			th CreationDate
			th LastUpdateDate
		
			tr
				td= masterorder.id
				td= masterorder.orderRefNumber
				td= masterorder.customerName
				td= masterorder.customerAccountId
				td= masterorder.orderStatus
				td= masterorder.creationDate
				td= masterorder.lastUpdateDate
